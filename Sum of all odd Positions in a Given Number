import java.util.*;
class Main
{
    public static void main(String []args)
    {
       Scanner s=new Scanner(System.in);
       int a=s.nextInt();
       int b=0,c=0,d=0;
       if(a<=0)
       {
           System.out.println("Invalid Input");
       }
       else
       {
          while(a>0)
          {
              b=a%10;
              a=a/10;
              c++;
          if(c%2==1)
          {
              d=d+b;
          }
          }
          System.out.print(d);
       }
    }
}
